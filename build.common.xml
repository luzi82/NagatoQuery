<?xml version="1.0" encoding="UTF-8"?>
<project name="nagatoquerycommon" default="none" basedir=".">

	<target name="none" />

	<dirname property="nagatoquerycommon.basedir" file="${ant.file.nagatoquerycommon}"/>

	<property file="${user.home}/.ant/user.properties" />
	<property file="${nagatoquerycommon.basedir}/project.properties" />
	<property name="nq.src.dir" value="${nagatoquerycommon.basedir}/NagatoQuery/src" />
	<property name="democommon.src.dir" value="${nagatoquerycommon.basedir}/NagatoQueryDemo-common/src" />

	<condition property="nq.full.version" value="${nq.main.version}.${env.BUILD_VERSION}" else="${nq.main.version}">
		<isset property="env.BUILD_VERSION" />
	</condition>

</project>

